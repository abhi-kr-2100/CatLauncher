{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cat-launcher",
  "version": "0.2.0",
  "identifier": "com.munetmo.cat-launcher",
  "build": {
    "beforeDevCommand": "cargo test --manifest-path ./src-tauri/Cargo.toml && pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cargo test --manifest-path ./src-tauri/Cargo.toml && pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "cat-launcher",
        "width": 850,
        "height": 650
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
