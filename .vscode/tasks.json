{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "tauri:dev-server",
      "type": "shell",
      "command": "pnpm",
      "args": ["dev"],
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": ".*",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": "cat-launcher",
            "endsPattern": "ready"
          }
        }
      ],
      "group": {
        "kind": "none"
      },
      "options": {
        "cwd": "${workspaceFolder}/cat-launcher"
      }
    }
  ]
}
